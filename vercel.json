{
    "rewrites": [
      { "source": "/", "destination": "/index.html" },
      { "source": "/dashboard", "destination": "/src/pages/dashboard.html" },
      { "source": "/terms-of-service", "destination": "/src/pages/terms.html" },
      { "source": "/privacy-policy", "destination": "/src/pages/privacy.html" },
      { "source": "/demo-data/(.*)", "destination": "/demo-data/$1" },
      { "source": "/assets/(.*)", "destination": "/assets/$1" }
    ]
}